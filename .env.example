# ==================== OKX API配置 ====================
# 从OKX交易所申请：https://www.oyuzh.com/zh-hans/account/my-api?shortCode=6CngT5 (https://www.okx.com/account/my-api?shortCode=6CngT5)
OKX_API_KEY=your_api_key_here
OKX_SECRET_KEY=your_secret_key_here
OKX_PASSPHRASE=your_passphrase_here

IS_DEMO=0

# ==================== 交易配置 ====================
# 交易标的（永续合约）
INST_ID=BTC-USDT-SWAP

# 杠杆倍数（1-125，建议20以下）
DEFAULT_LEVERAGE=20

# AI轮询间隔时间(分钟)
INTERVAL_MINUTES=5.0

# 机器人启动时间（用于计算运行时长）格式：YYYY-MM-DD HH:MM:SS
BOT_START_TIME=2025-01-01 00:00:00

# ==================== AI配置 ====================
# AI提供商选择：doubao（豆包）/ deepseek / qwen（通义千问）
AI_PROVIDER=doubao

# 豆包（字节跳动）配置
DOUBAO_API_KEY=
DOUBAO_ENDPOINT_ID=doubao-seed-1-6-251015
DOUBAO_MODEL=doubao-seed-1-6-251015
DOUBAO_TIMEOUT=60

# DeepSeek配置
DEEPSEEK_API_KEY=
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_TIMEOUT=60

# 通义千问（阿里云）配置
QWEN_API_KEY=
QWEN_MODEL=qwen-plus
QWEN_TIMEOUT=60

# ==================== 数据库配置 ====================
# MySQL配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=abc123456
MYSQL_DATABASE=trading_data

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# ==================== 代理配置 ====================
# 是否启用代理（国内访问OKX需要代理）
PROXY_ENABLED=false

# 代理地址
PROXY_TYPE=http
PROXY_HOST=127.0.0.1
PROXY_PORT=7890

# 代理认证（如果代理需要用户名密码）
PROXY_USERNAME=
PROXY_PASSWORD=

# ==================== 飞书通知配置（可选）====================
# 是否启用飞书通知
FEISHU_ENABLED=false

# 飞书机器人Webhook地址
FEISHU_WEBHOOK_URL=

# ==================== 日志配置 ====================
# 日志级别：DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# 日志保留天数
LOG_RETENTION_DAYS=7



#数据采集器配置
COLLECTOR_SYMBOLS=BTC-USDT-SWAP,ETH-USDT-SWAP
COLLECTOR_TIMEFRAMES=1m,5m,15m,30m,1H,4H
COLLECTOR_HISTORY_DAYS=30
COLLECTOR_STATUS_INTERVAL=20
COLLECTOR_DATA_TIMEOUT=180
